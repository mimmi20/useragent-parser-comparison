{
    "name": "diablomedia/useragent-parser-comparison",
    "description": "project to compare some useragent parsers",
    "license": "MIT",
    "type": "library",
    "homepage": "https://github.com/diablomedia/useragent-parser-comparison",
    "require": {
        "php": "~8.3.0 || ~8.4.0 || ~8.5.0",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-simplexml": "*",
        "composer-runtime-api": "^2.0",
        "mimmi20/browscap-helper-source": "^5.0.3",
        "ramsey/uuid": "^4.7.6",
        "symfony/console": "^7.2.1"
    },
    "require-dev": {
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-tokenizer": "*",
        "ext-xml": "*",
        "ext-xmlwriter": "*",
        "infection/infection": "^0.29.10",
        "mimmi20/coding-standard": "^6.0.1",
        "nikic/php-parser": "^5.3.1",
        "phpstan/extension-installer": "^1.4.3",
        "phpstan/phpstan": "^1.12.9",
        "phpstan/phpstan-deprecation-rules": "^1.2.1",
        "rector/rector": "^1.2.10",
        "rector/type-perfect": "^1.0.0",
        "shipmonk/composer-dependency-analyser": "^1.8.1",
        "symfony/process": "^7.2.0",
        "symplify/phpstan-rules": "^13.0.1",
        "tomasvotruba/cognitive-complexity": "^0.2.3",
        "tomasvotruba/type-coverage": "^1.0.0",
        "tomasvotruba/unused-public": "^1.1.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "UserAgentParserComparison\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true,
            "composer/package-versions-deprecated": true,
            "infection/extension-installer": true
        }
    },
    "scripts": {
        "php-cs-fixer-check": "php-cs-fixer fix --dry-run -vv",
        "php-cs-fixer-fix": "php-cs-fixer fix -vv",
        "phpstan": "phpstan analyse -c phpstan.neon --memory-limit=768M --no-progress",
        "prettier-check": "yarn run prettier -l \"**/*.js\"",
        "prettier-fix": "yarn run prettier --write \"**/*.js\"",
        "psalm": "psalm --show-info=false"
    }
}
